{
  "compilerOptions": {
    "moduleDetection": "force",
    "target": "ESNext",
    "module": "Preserve",
    "esModuleInterop": true,
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "paths": {
      "@models/*": ["src/models/*"],
      "@server/*": ["src/server/*"],
    }
  }
}